# Server Configuration
PORT=3000
FRONTEND_URL=http://localhost:5173

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Set NODE_ENV (development/production)
NODE_ENV=development

# Optional: Max file size limit (in bytes, default is 5MB)
MAX_FILE_SIZE=5242880

# Optional: Max number of versions per project
MAX_VERSIONS=10

# Optional: Max number of projects to store
MAX_PROJECTS=100